

  <% if( theme.style == 'blog') { %>
  <h1><%- config.archive -%>Simon Online</h1>
    <%- partial('_partial/archive', {pagination: config.archive, is_index: true}) %>
  <% } else { %>

    <!-- Two -->
    <section id="two">
      <h2>Recent Work</h2>
      <div class="row">
        <% page.posts.forEach(function(item, index){ %>
            <%- partial('_partial/article-index', {item: item, index: index}) %>
        <% }); %>
      </div>

  <% } %>

    <%- partial('_partial/pagination') %>

    <ul class="actions">
      <li><a href="<%- url_for('archives') -%>" class="button">Archives</a></li>
    </ul>
  </section>
