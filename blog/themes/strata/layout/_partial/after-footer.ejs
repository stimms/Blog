  <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.10.0/languages/fsharp.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.14.2/languages/javascript.min.js" integrity="sha256-aP+yiN6DRVl0yk7Pb9pGIOKVDSyIEtFWTduEEteeD+0=" crossorigin="anonymous"></script>
  <script>hljs.initHighlightingOnLoad();</script>
  <% } %>
